{
  "$schema": "http://json.schemastore.org/template",
  "author": "megacheliq",
  "classifications": [
    "Web",
    "Multi-Project",
    "ASP.NET Core"
  ],
  "identity": "TelegramTemplate",
  "name": "Template for tg bot on .NET",
